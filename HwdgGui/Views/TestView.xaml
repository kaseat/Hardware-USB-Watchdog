        <StackPanel>
            <Button Name="SoftResetTest" Margin="5" Focusable="False" Content="Тест перезагрузки"/>
            <Button Name="HardResetTest" Margin="5" Focusable="False" Content="Тест принудительной перезагрузки"/>
